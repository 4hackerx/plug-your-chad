{
  "name": "vscode-extension",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "out/extension.cjs",
  "scripts": {
    "build:interface": "tsc && vite build --config vite.interface.config.js --emptyOutDir",
    "build:extension": "tsc && vite build --config vite.extension.config.js",
    "build": "npm run build:interface && npm run build:extension",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "pack": "vsce package --yarn"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "vscode-plug-your-chad.showChadDialog",
        "title": "Show Chad Dialog",
        "category": "Plug Your Chad"
      },
      {
        "command": "vscode-plug-your-chad.copyLines",
        "title": "Copy Lines",
        "category": "Plug Your Chad"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vscode-plug-your-chad.showChadDialog"
        },
        {
          "command": "vscode-plug-your-chad.copyLines"
        }
      ]
    }
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@tsparticles/react": "^3.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.372.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vscode-webview-dialog": "github:kelvinpraises/vscode-webview-dialog"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
